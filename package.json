{
  "name": "@snowcoders/react-popper",
  "version": "0.7.94",
  "description": "React wrapper around PopperJS.",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.dist.config.js",
    "build-example": "webpack --config webpack.example.config.js",
    "clean": "rimraf coverage dist dist-example",
    "prepare": "npm run clean && npm run build",
    "prettier": "prettier --write \"{src,example}/**/*\"",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "serve": "live-server --port=9090 --host=localhost --watch=dist,dist-example,index.html",
    "test": "npm run test-karma",
    "test-karma": "karma start ./karma.conf.js",
    "watch": "concurrently \"npm run watch-src\" \"npm run watch-example\"",
    "watch-src": "npm run build -- --watch",
    "watch-example": "npm run build-example -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snowcoders/react-popper"
  },
  "keywords": [
    "react",
    "react-popper",
    "popperjs",
    "component",
    "drop",
    "tooltip",
    "popover"
  ],
  "author": "snowcoders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowcoders/react-popper/issues"
  },
  "homepage": "https://github.com/snowcoders/react-popper",
  "peerDependencies": {
    "react": ">=15.4.0",
    "react-dom": ">=15.4.0",
    "tslib": ">=1.6.0",
    "popper.js": ">=1.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/sinon": "^4.1.2",
    "@types/webpack": "^3.8.1",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.5.1",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-jsdom-launcher": "^6.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-typescript": "^3.0.9",
    "karma-typescript-es6-transform": "^1.0.3",
    "karma-typescript-preprocessor": "^0.3.1",
    "live-server": "^1.2.0",
    "mocha": "^4.1.0",
    "mocha-typescript": "^1.1.12",
    "node-sass": "^4.7.2",
    "popper.js": "^1.13.0-next.1",
    "prettier": "1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "tslib": "^1.9.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
